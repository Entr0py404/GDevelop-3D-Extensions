    {
      "description": "Change the color of all materials (default is white).",
      "fullName": "Color",
      "functionType": "Action",
      "group": "material",
      "name": "Color",
      "sentence": "Change the color of _PARAM1_ materials to _PARAM2_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const rgbcolor = gdjs.rgbOrHexToRGBColor(eventsFunctionContext.getArgument(\"Color\"));",
            "const hexColor = \"0x\" + gdjs.rgbToHex(rgbcolor[0], rgbcolor[1], rgbcolor[2]);",
            "objects[0].get3DRendererObject().traverse((node) => {",
            "    if (node instanceof THREE.Mesh && node.material) {",
            "        node.material.color.setHex(hexColor);",
            "    }",
            "});"
          ],
          "parameterObjects": "Object",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "parameters": [
        {
          "description": "3D model",
          "name": "Object",
          "supplementaryInformation": "Scene3D::Model3DObject",
          "type": "objectList"
        },
        {
          "description": "Color",
          "name": "Color",
          "type": "color"
        }
      ],
      "objectGroups": []
    },
