    {
      "description": "Change color by material slot (default is white).",
      "fullName": "Color by material slot",
      "functionType": "Action",
      "group": "material by slot",
      "name": "ColorBySlot",
      "sentence": "Change the color of _PARAM1_ material in slot _PARAM3_ to _PARAM2_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "const rgbcolor = gdjs.rgbOrHexToRGBColor(eventsFunctionContext.getArgument(\"Color\"))",
            "const hexColor = \"0x\" + gdjs.rgbToHex(rgbcolor[0], rgbcolor[1], rgbcolor[2]);",
            "const slotNum = eventsFunctionContext.getArgument(\"Slot\");",
            "let index = 0;",
            "objects[0].get3DRendererObject().traverse((node) => {",
            "    if (index <= slotNum && node instanceof THREE.Mesh && node.material) {",
            "        if (index === slotNum) {",
            "            node.material.color.setHex(hexColor);",
            "        }",
            "        index++;",
            "    }",
            "});"
          ],
          "parameterObjects": "Object",
          "useStrict": true,
          "eventsSheetExpanded": false
        }
      ],
      "parameters": [
