    {
      "description": "Convert material type",
      "fullName": "Convert material",
      "functionType": "Action",
      "group": "material",
      "name": "ConvertMaterialType",
      "sentence": "Convert material type of _PARAM1_ materials to _PARAM2_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "objects[0].get3DRendererObject().traverse((node) => {",
            "    if (node instanceof THREE.Mesh && node.material) {",
            "        let targetMaterial;",
            "        switch (eventsFunctionContext.getArgument(\"MaterialType\")) {",
            "            case 'Basic':",
            "                targetMaterial = new THREE.MeshBasicMaterial();",
            "                break;",
            "            case 'Lambert':",
            "                targetMaterial = new THREE.MeshLambertMaterial();",
            "                break;",
            "            case 'Phong':",
            "                targetMaterial = new THREE.MeshPhongMaterial();",
            "                break;",
            "            case 'Toon':",
            "                targetMaterial = new THREE.MeshToonMaterial();",
            "                break;",
            "            case 'Standard':",
            "                targetMaterial = new THREE.MeshStandardMaterial();",
            "                break;",
            "            case 'Normal':",
            "                targetMaterial = new THREE.MeshNormalMaterial();",
            "                break;",
            "            case 'Depth':",
            "                targetMaterial = new THREE.MeshDepthMaterial();",
            "                break;",
            "            case 'Shadow':",
            "                targetMaterial = new THREE.ShadowMaterial();",
            "                break;",
            "            default:",
            "                console.error('Unsupported target material type');",
            "                break;",
            "        }",
            "",
            "        targetMaterial.blending = node.material.blending;",
            "        targetMaterial.side = node.material.side;",
            "        targetMaterial.vertexColors = node.material.vertexColors;",
            "        targetMaterial.opacity = node.material.opacity;",
            "        targetMaterial.transparent = node.material.transparent;",
            "        targetMaterial.blendSrc = node.material.blendSrc;",
            "        targetMaterial.blendDst = node.material.blendDst;",
            "        targetMaterial.blendEquation = node.material.blendEquation;",
