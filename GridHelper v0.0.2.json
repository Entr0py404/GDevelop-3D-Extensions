    {
      "description": "Create a grid helper.",
      "fullName": "Create grid helper",
      "functionType": "Action",
      "name": "CreateGridHelper",
      "sentence": "Create grid helper _PARAM1_, size: _PARAM2_, divisions: _PARAM3_, center line color: _PARAM4_, color grid: _PARAM5_, position X: _PARAM6_, Y: _PARAM7_, Z: _PARAM8_",
      "events": [
        {
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": [
            "if (!runtimeScene._gridHelperExtension.gridHelpers.has(eventsFunctionContext.getArgument(\"Identifier\"))) {",
            "const rgbColorGD1 = gdjs.rgbOrHexToRGBColor(eventsFunctionContext.getArgument(\"ColorCenterLine\"));",
            "const rgbColor_ColorCenterLine = new THREE.Color(\"rgb(\" + rgbColorGD1[0] + \", \" + rgbColorGD1[1] + \", \" + rgbColorGD1[2] + \")\");",
            "const rgbColorGD2 = gdjs.rgbOrHexToRGBColor(eventsFunctionContext.getArgument(\"ColorGrid\"));",
            "const rgbColor_ColorGrid = new THREE.Color(\"rgb(\" + rgbColorGD2[0] + \", \" + rgbColorGD2[1] + \", \" + rgbColorGD2[2] + \")\");",
            "const gridHelper = new THREE.GridHelper(eventsFunctionContext.getArgument(\"Size\"), eventsFunctionContext.getArgument(\"Divisions\"), rgbColor_ColorCenterLine, rgbColor_ColorGrid);",
            "gridHelper.rotation.x = Math.PI / 2;",
            "gridHelper.position.x = eventsFunctionContext.getArgument(\"PositionX\");",
            "gridHelper.position.y = eventsFunctionContext.getArgument(\"PositionY\");",
            "gridHelper.position.z = eventsFunctionContext.getArgument(\"PositionZ\");",
            "runtimeScene.getLayer('').getRenderer().getThreeScene().add(gridHelper);",
            "runtimeScene._gridHelperExtension.gridHelpers.set(eventsFunctionContext.getArgument(\"Identifier\"), { gridHelper });",
            "gridHelper.dispose();",
            "}"
          ],
          "parameterObjects": "",
          "useStrict": true,
          "eventsSheetExpanded": true
        }
      ],
      "parameters": [
        {
          "description": "Identifier",
          "name": "Identifier",
          "type": "string"
        },
        {
          "description": "The size of the grid (default is 10)",
          "name": "Size",
          "type": "expression"
        },
        {
          "description": "The number of divisions across the grid (default is 10)",
          "name": "Divisions",
          "type": "expression"
        },
        {
          "description": "The color of the centerline",
          "name": "ColorCenterLine",
          "type": "color"
        },
        {
          "description": "The color of the lines of the grid",
          "name": "ColorGrid",
          "type": "color"
        },
        {
          "description": "X position",
          "name": "PositionX",
          "type": "expression"
